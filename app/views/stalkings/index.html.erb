<h1>Listing stalkings</h1>

<div class="row-fluid stalkings">
  <div class="span5">
  <% @stalkings.each do |stalking| %>
    <p>
      <%= link_to '<i class="icon-remove"></i>'.html_safe, stalking, method: :delete, data: { confirm: 'Are you sure you want to delete this Stalking?' }, title: 'Delete' %>
      <a href="<%= edit_stalking_path(stalking) %>" title="Edit"><i class="icon-edit"></i></a>
      <a href="https://github.com/<%= stalking.owner %>/<%= stalking.repo %>" target="blank" class="repo"><%= stalking.owner %>/<%= stalking.repo %></a>
      <button class="btn btn-success stalk">Stalk</button>
      <img class="loading" src="/assets/ajax-loader.gif" alt="Loading..." />
    </p>
  <% end %>
  <p>
    <a href="<%= new_stalking_path %>"><button class="btn btn-primary">New Stalking</button></a>
  </p>
  </div>
  <div class="span7 results">
    <h2></h2>
    <div class="commits"></div>
  </div>
</div>